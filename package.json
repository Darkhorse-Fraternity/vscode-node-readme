{
  "name": "vscode-node-readme",
  "displayName": "node-readme",
  "description": "view installed node_modules readmes",
  "version": "1.1.0",
  "publisher": "bengreenier",
  "icon": "images/icon.svg",
  "engines": {
        "vscode": "^1.12.0"
    },
  "galleryBanner": {
    "color": "#ffc985",
    "theme": "light"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:nodeReadme.showReadme"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == javascript",
          "command": "nodeReadme.showReadme",
          "group": "navigation@9999"
        },
        {
          "when": "editorLangId == typescript",
          "command": "nodeReadme.showReadme",
          "group": "navigation@9999"
        }
      ]
    },
    "commands": [
      {
        "title": "View Node Module Readme",
        "command": "nodeReadme.showReadme"
      }
    ],
    "keybindings": [{
        "command": "nodeReadme.showReadme",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus"
    }]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "node-core-module-names": "^1.1.0",
    "request": "^2.74.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bengreenier/vscode-node-readme.git"
  },
  "keywords": [
    "vscode",
    "node",
    "readme"
  ],
  "author": "Ben Greenier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bengreenier/vscode-node-readme/issues"
  },
  "homepage": "https://github.com/bengreenier/vscode-node-readme#readme"
}
